import { Autowired } from '@exteranto/ioc'
import { Browser } from '@exteranto/support'
import { Comb } from '@exteranto/compatibility'

class OnlyChromeFunctionality {

  @Autowired
  private comb: Comb

  public execute () : void {
    this.comb.only([Browser.CHROME], () => {
      // Only chrome functionality.
    }).thenOnly([Browser.CHROME], () => {
      // Both functions are interchangeable.
    })
  }
}
