import { Autowired } from '@exteranto/ioc'
import { Browser } from '@exteranto/support'
import { Comb } from '@exteranto/compatibility'

class SomeService {

  @Autowired
  private comb: Comb

  public execute () : void {
    this.comb.only([Browser.CHROME], () => {
      return 1
    }).thenExcept([Browser.CHROME], () => {
      return 2
    }).then((result) => {
      // if current browser is chrome, assert result === 1
      // else assert result === 2
    })
  }
}
