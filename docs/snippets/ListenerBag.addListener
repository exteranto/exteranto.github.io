import { Autowired } from '@exteranto/ioc'
import { Dispatcher } from '@exteranto/events'
import { MyListener } from './MyListener'

class SomeService {

  @Autowired
  private dispatcher: Dispatcher

  public execute () : void {
    // Note that you are advised against using this method directly. You should
    // leverage the event router instead.
    this.dispatcher.touch('my.event').addListener(MyListener)
  }
}
