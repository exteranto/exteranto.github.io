import config from 'config'
import events from './events'
import { App } from '@exteranto/core'
import { Script } from '@exteranto/support'

const app: App = new App(
  Script.BACKGROUND,
  config,
  events
)

/**
 * Dispatches the `app.booted` event upon completion. Do not put any logic prior
 * to calling this function as the application services are yet to be booted.
 */
app.bootstrap()
